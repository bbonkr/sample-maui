<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:viewModel="clr-namespace:SampleLayoutApp.ViewModels"
             xmlns:view="clr-namespace:SampleLayoutApp.Views"
             x:DataType="viewModel:LayoutsViewModel"
             x:Class="SampleLayoutApp.Views.LayoutsPage"
             Title="Layouts">
    <TableView Intent="Menu">
        <TableRoot>
            <TableSection>
                <TextCell Text="FlexLayout"
                          Command="{Binding GoToFlexLayoutCommand}"
                          Detail="FlexLayout direction change does not work"/>
            </TableSection>
        </TableRoot>
    </TableView>
</ContentPage>
